{
  "name": "five-gestures-game",
  "homepage": "https://avonavi232.github.com/five-gestures-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.3",
    "gh-pages": "^1.2.0",
    "material-ui": "^0.20.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-svg": "^3.0.6",
    "react-test-renderer": "^16.4.1",
    "snapsvg": "0.5.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "static_html": "cd src && browser-sync start -s -f \"views/*.html, styles/*.css\" --port 5432 --startPath \"views/initial_screen.html\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "deep-freeze": "0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewired": "^1.5.2"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./src/__tests__/setup/fileMock.js",
      "\\.(css|less)$": "./src/__tests__/setup/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "./src/__tests__/setup/setup.js",
    "modulePathIgnorePatterns": [
      "setup.js",
      "styleMock",
      "fileMock"
    ],
    "collectCoverage": false
  }
}
